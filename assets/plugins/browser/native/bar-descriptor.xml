        <text xml:lang="gl"> Explorador</text>
        <text xml:lang="he"> דפדפן</text>
        <text xml:lang="hi">ब ्राउज़र</text>
        <text xml:lang="hr"> Preglednik</text>
        <text xml:lang="hu"> Böngésző</text>
        <text xml:lang="id"> Browser</text>
        <text xml:lang="it"> Browser</text>
        <text xml:lang="ja"> ブラウザー</text>
        <!-- start pseudo locale -->
        <text xml:lang="kl"> klполн</text>
        <!-- end pseudo locale -->
        <text xml:lang="ko"> 브라우저</text>
        <text xml:lang="ms"> Pelayar</text>
        <text xml:lang="nb"> Nettleser</text>
        <text xml:lang="nl"> Browser</text>
        <text xml:lang="pl"> Przeglądarka</text>
        <text xml:lang="pt"> Browser</text>
        <text xml:lang="pt-BR"> Navegador</text>
        <text xml:lang="ro"> Browser</text>
        <text xml:lang="ru"> Браузер</text>
        <text xml:lang="sv"> Webbläsare</text>
        <text xml:lang="th"> เบราเซอร์</text>
        <text xml:lang="tr"> Tarayıcı</text>
