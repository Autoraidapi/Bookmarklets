        <text xml:lang="zh-CN"> 浏览器</text>
        <text xml:lang="zh-TW"> 瀏覽器</text>
    </name>
    <icon>
        <image>app_icon_browser.png</image>
    </icon>
    <versionNumber>1.0.0</versionNumber>
    <buildId>0</buildId>
    <packageId></packageId>
    <description> Browser</description>
    <author></author>
    <asset type="Qnx/Elf" entry="true">browser</asset>
    <asset public="true">bing.png</asset>
    <asset public="true">google.png</asset>
    <asset public="true">yahoo.png</asset>
	
    <entryPointType>Qnx/WebKit</entryPointType>
	
    <initialWindow>
        <autoOrients>true</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>true</transparent>
    </initialWindow>
	
    <env var="WK_ENABLE_CLIENT_CERTS" value="1" />
	
    <permission>run_when_backgrounded</permission>
    <permission>access_location_services</permission>
    <permission>use_camera</permission>
    <permission>record_audio</permission>
    <permission>access_shared</permission>
    <permission>access_internet</permission>
    <permission>post_notification</permission>
    <permission>gain_oma_fl_group</permission>
    <permission>access_oma_fl_write_personal</permission>
    <permission>access_oma_fl_write_enterprise</permission>
    <permission>access_bbjma_data</permission>
    <permission>access_carrier_browser</permission>
    <permission>access_cclagent_service</permission>
    <permission>access_dialog_system_control</permission>
    <permission>use_certmgr_server</permission>
    <permission>access_wifi_limited</permission>
    <permission>access_managed_credentials</permission>
    <permission>read_multimedia_settings</permission>
	
    <splashScreens>
        <image>splash_1280x768.png</image>
        <image>splash_768x1280.png</image>
        <image>splash_720x720.png</image>
        <image>splash_720x1280.png</image>
        <image>splash_1280x720.png</image>
        <image>splash_1080x1920.png</image>
        <image>splash_1920x1080.png</image>
        <image>splash_1440x1440.png</image>
    </splashScreens>
	
    <invoke-target id="com.browser">
        <invoke-target-type>application</invoke-target-type>
        <icon>
            <image>ca_browser.png</image>
        </icon>
        <filter>
            <action>bb.action.OPEN</action>
            <mime-type>*</mime-type>
            <property var="uris" value="http:,https:,ftp:,rtsp:,websearch:"/>
        </filter>
        <filter>
            <action>bb.action.OPEN</action>
            <mime-type>text/html</mime-type>
            <mime-type>application/xhtml+xml</mime-type>
            <mime-type>image/svg+xml</mime-type>
            <mime-type>application/x-shockwave-</mime-type>
			<mime-type>video/x-flv</mime-type>
            <property var="uris" value="file://,asset://"/>
            <property var="exts" value="htm,html,xhtml,svg,swf,flv"/>
        </filter>
    </invoke-target>
	
    <invoke-target id="com.browser.personal">
        <invoke-target-type>application</invoke-target-type>
        <invoke-target-name>
            <text xml:lang="en"> Personal Browser</text>
            <text xml:lang="ar"> المستعرض الشخصي</text>
            <text xml:lang="ca"> Navegador personal</text>
            <text xml:lang="cs"> Osobní prohlížeč</text>
            <text xml:lang="da"> Personlig Browser</text>
            <text xml:lang="de"> Privat-Browser</text>
            <text xml:lang="el"> Προσωπικός Περιηγητής</text>
            <text xml:lang="en-GB"> Personal Browser</text>
            <text xml:lang="es"> Navegador personal</text>
            <text xml:lang="eu"> Arakatzaile pertsonala</text>
            <text xml:lang="fi"> Hlökohtainen selain</text>
            <text xml:lang="fr"> Navigateur personnel</text>
            <text xml:lang="gl"> Explorador persoal</text>
            <text xml:lang="he"> דפדפן אישי</text>
            <text xml:lang="hi"> व्यक्तिगत ब्राउज़र</text>
            <text xml:lang="hr"> Lični Preglednik</text>
            <text xml:lang="hu"> Személyes Böngésző</text>
            <text xml:lang="id"> Penjelajah Pribadi</text>
            <text xml:lang="it"> Browser personale</text>
            <text xml:lang="ja"> 個人的なブラウザー</text>
            <!-- start pseudo locale -->
            <text xml:lang="kl"> Personal klполнr</text>
            <!-- end pseudo locale -->
            <text xml:lang="ko"> 개인용 브라우저</text>
            <text xml:lang="nb"> Personlig Nettleser</text>
            <text xml:lang="nl"> Persoonlijke browser</text>
            <text xml:lang="pl"> Przeglądarka osobista</text>
            <text xml:lang="pt"> Navegador Pessoal</text>
            <text xml:lang="pt-BR"> Navegador Pessoal</text>
            <text xml:lang="ro"> Browser personal</text>
            <text xml:lang="ru"> Личный Браузер</text>
            <text xml:lang="sv"> Personlig Webbläsare</text>
            <text xml:lang="th"> เบราเซอร์ ส่วนตัว</text>
            <text xml:lang="tr"> Kişisel Tarayıcı</text>
            <text xml:lang="vi"> Trình duyệt cá nhân </text>
            <text xml:lang="zh-CN"> 个人浏览器</text>
            <text xml:lang="zh-TW"> 個人瀏覽器</text>
        </invoke-target-name>
	    
        <icon>
            <image>ca_browser.png</image>
        </icon>
	    
        <filter>
            <action>bb.action.OPEN</action>
            <mime-type>*</mime-type>
            <property var="uris" value="http:,https:,ftp:,rtsp:"/>
            <property var="perimeter" value="enterprise"/>
        </filter>	
	    
    </invoke-target>
	<feature id="blackberry.io" required="true"/>
	<feature id="blackberry.io.home" required="true"/>
	<feature id="blackberry.io.file" required="true"/>
	
</qnx>
